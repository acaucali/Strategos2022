<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping SYSTEM "../../../../../../hibernate-mapping-3.0.dtd" >

<!-- DescripciÃ³n: Mapeo de un objeto Codigo de Enlace de la Base de Datos -->
<!-- Modificado por: Kerwin Arias (08/04/2012) -->

<hibernate-mapping>
	<class name="com.visiongc.app.strategos.modulo.codigoenlace.model.CodigoEnlace" table="pgn_enlace">

		<!-- Atributo clave de id  -->
		<id name="id" type="java.lang.Long" column="id">
			<generator class="assigned" />
		</id>

		<!-- Atributos del objeto conceptual para Codigo de Enlace  -->
		<property name="codigo" type="java.lang.String" column="enlace_tipo" />
		<property name="nombre" type="java.lang.String" column="enlace_descripcion" />
		<property name="bi" type="java.lang.Long" column="bi" />
		<property name="categoria" type="java.lang.Long" column="categoria" />

	</class>

</hibernate-mapping>