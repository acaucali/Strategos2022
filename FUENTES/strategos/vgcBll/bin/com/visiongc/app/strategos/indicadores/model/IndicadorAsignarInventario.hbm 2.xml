<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping SYSTEM "file:///hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

	<class name="com.visiongc.app.strategos.indicadores.model.IndicadorAsignarInventario" table="indicador_asignar_inventario" lazy="true">
		
		<id name="asignarId" type="java.lang.Long" column="asignar_id">
			<generator class="assigned" />
		</id>
		
		<!-- Atributos  -->
		<property name="indicadorId" type="java.lang.Long" column="indicador_id" />
		
		<!-- Atributos  -->
		<property name="indicadorInsumoId" type="java.lang.Long" column="indicador_insumo_id" />
		
		
	</class>

</hibernate-mapping>