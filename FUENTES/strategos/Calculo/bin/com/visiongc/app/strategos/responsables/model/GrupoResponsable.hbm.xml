<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping SYSTEM "../../../../../../hibernate-mapping-3.0.dtd" >

<!-- Descripción: Mapeo de un objeto GrupoResponsable de la Base de Datos -->
<!-- Modificado por: Kerwin Arias (28/10/2012) -->

<hibernate-mapping>

	<class name="com.visiongc.app.strategos.responsables.model.GrupoResponsable" table="responsable_grupo">

		<!-- Composición de claves  -->
		<composite-id name="pk" class="com.visiongc.app.strategos.responsables.model.GrupoResponsablePK">
			<key-property name="padreId" column="padre_id" type="java.lang.Long" />
			<key-property name="responsableId" column="responsable_id" type="java.lang.Long" />
		</composite-id>

	</class>

</hibernate-mapping>
