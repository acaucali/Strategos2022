<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping SYSTEM "../../../../../../hibernate-mapping-3.0.dtd" >

<!-- DescripciÃ³n: Mapeo de un objeto Reporte de la Base de Datos -->
<!-- Creado por: Andres Caucali (08/08/2021) -->

<hibernate-mapping>
	<class name="com.visiongc.app.strategos.instrumentos.model.Cooperante" table="cooperante">

		<!-- Atributo clave de responsable -->
		<id name="cooperanteId" type="java.lang.Long" column="cooperante_id">
			<generator class="assigned" />
		</id>

		<!-- Atributos del objeto conceptual para  responsable -->
		<property name="nombre" type="java.lang.String" column="nombre" length="50"/>
		<property name="descripcion" type="java.lang.String" column="descripcion" length="150"/>
		<property name="pais" type="java.lang.String" column="pais" length="150"/>
		
		
	</class>
</hibernate-mapping>
