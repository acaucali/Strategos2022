<hibernate-mapping>
	<class name="com.visiongc.app.strategos.iniciativas.model.util.TipoProyecto" table="tipo_proyecto">

		<!-- Atributo clave de Categoria -->
		<id name="tipoProyectoId" type="java.lang.Long" column="tipo_proyecto_id">
			<generator class="assigned" />
		</id>

		<!-- Atributos del objeto conceptual para Categoria -->
		<property name="nombre" type="java.lang.String" column="nombre_proyecto" />

	</class>

</hibernate-mapping>