<?xml version='1.0' encoding='utf-8'?>

<!DOCTYPE hibernate-mapping SYSTEM "../../../../hibernate-mapping-3.0.dtd" >

<!-- Creado por: Kerwin Arias (14/05/2012) -->

<hibernate-mapping>

	<class name="com.visiongc.framework.model.Version" table="afw_version">

		<composite-id>
			<key-property name="version" column="version" type="java.lang.String" length="50" />
			<key-property name="build" column="build" type="java.lang.String" length="8" />
			<key-property name="dateBuild" column="dateBuild" type="java.lang.String" length="50" />
		</composite-id>

		<property name="createdAt" column="createdAt" type="java.sql.Timestamp" length="8" />
	</class>

</hibernate-mapping>
