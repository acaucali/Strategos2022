<?xml version='1.0' encoding='utf-8'?>

<!DOCTYPE hibernate-mapping SYSTEM "../../../../../hibernate-mapping-3.0.dtd"  >

<!-- Modificado por: Kerwin Arias (06/04/2012) -->

<hibernate-mapping>

	<class name="com.visiongc.framework.auditoria.model.AuditoriaMedicion" table="auditoria_medicion">

		<id name="auditoriaMedicionId" type="java.lang.Long" column="auditoria_medicion_id">
			<generator class="assigned" />
		</id>
		
		<property name="sesion" type="java.lang.String" column="sesion" />
		<property name="fechaEjecucion" type="java.util.Date" column="fecha_ejecucion" />
		<property name="accion" type="java.lang.String" column="accion" />
		<property name="organizacion" type="java.lang.String" column="organizacion" />
		<property name="organizacionId" type="java.lang.Long" column="organizacion_id" />
		<property name="periodo" type="java.lang.String" column="periodo" />
		<property name="periodoFinal" type="java.lang.String" column="periodo_final" />
		<property name="usuario" type="java.lang.String" column="usuario" />
		<property name="indicador" type="java.lang.String" column="indicador" />
		<property name="clase" type="java.lang.String" column="clase" />
		<property name="iniciativa" type="java.lang.String" column="iniciativa" />
		<property name="indicadorId" type="java.lang.Long" column="indicador_id" />
		<property name="detalle" type="java.lang.String" column="detalle" />
		
	</class>
</hibernate-mapping>
