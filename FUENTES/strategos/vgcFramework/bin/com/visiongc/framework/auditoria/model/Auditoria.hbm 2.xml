<?xml version='1.0' encoding='utf-8'?>

<!DOCTYPE hibernate-mapping SYSTEM "../../../../../hibernate-mapping-3.0.dtd"  >

<!-- Modificado por: Kerwin Arias (06/04/2012) -->

<hibernate-mapping>

	<class name="com.visiongc.framework.auditoria.model.Auditoria" table="auditoria">

		<id name="auditoriaId" type="java.lang.Long" column="auditoria_id">
			<generator class="assigned" />
		</id>
		
		<property name="fechaEjecucion" type="java.util.Date" column="fecha_ejecucion" />
		<property name="usuario" type="java.lang.String" column="usuario" />
		<property name="entidad" type="java.lang.String" column="entidad" />
		<property name="claseEntidad" type="java.lang.String" column="clase_entidad" />
		<property name="tipoEvento" type="java.lang.String" column="tipo_evento" />
		<property name="detalle" type="java.lang.String" column="detalle" />
		
	</class>
</hibernate-mapping>
