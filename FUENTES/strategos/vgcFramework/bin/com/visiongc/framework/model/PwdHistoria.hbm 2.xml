<?xml version='1.0' encoding='utf-8'?>

<!DOCTYPE hibernate-mapping SYSTEM "../../../../hibernate-mapping-3.0.dtd" >

<!-- Modificado por: Juan Cruz (10/01/2013) -->

<hibernate-mapping>

	<class name="com.visiongc.framework.model.PwdHistoria" table="afw_pwd_historia">

		<composite-id>
			<key-property name="usuarioId" type="java.lang.Long" column="usuario_id" />
			<key-property name="pwd" type="java.lang.String" column="pwd" length="100" />
			<key-property name="fecha" type="java.sql.Timestamp" column="fecha" length="8" />
		</composite-id>
		
	</class>

</hibernate-mapping>
