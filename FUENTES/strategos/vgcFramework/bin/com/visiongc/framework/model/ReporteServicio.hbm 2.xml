<?xml version='1.0' encoding='utf-8'?>

<!DOCTYPE hibernate-mapping SYSTEM "../../../../hibernate-mapping-3.0.dtd" >

<!-- Modificado por: Kerwin Arias (28/08/2012) -->

<hibernate-mapping>

	<class name="com.visiongc.framework.model.ReporteServicio" table="reporte_servicio">
	
		<id name="reporteId" type="java.lang.Long" column="reporte_id">
			<generator class="assigned" />
		</id>
		
		<property name="responsableId" column="responsable_id" type="java.lang.Long" length="10" />		
		<property name="medicion" column="medicion" type="java.lang.String" />
		<property name="indicadorId" column="indicador_id" type="java.lang.Long" length="10" />
		
	</class>

</hibernate-mapping>
