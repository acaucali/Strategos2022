<?xml version='1.0' encoding='utf-8'?>

<!DOCTYPE hibernate-mapping SYSTEM "../../../../hibernate-mapping-3.0.dtd" >

<!-- Modificado por: Kerwin Arias (28/08/2012) -->

<hibernate-mapping>

	<class name="com.visiongc.framework.model.Transaccion" table="afw_transaccion">

		<id name="id" type="java.lang.Long" column="id"></id>

		<property name="nombre" type="java.lang.String" column="nombre" length="50" not-null="true"/>
		<property name="frecuencia" type="java.lang.Byte" column="frecuencia" length="1" not-null="true"/>
		<property name="configuracion" type="java.lang.String" column="configuracion" not-null="true" />
	</class>

</hibernate-mapping>
