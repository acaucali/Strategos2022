<hibernate-mapping>

	<class name="com.visiongc.framework.model.ObjetoSistema" table="afw_objeto_sistema">

		<id name="objetoId" type="java.lang.String" column="objeto_id" length="50">
			<generator class="assigned" />
		</id>


		<property name="articuloSingular" type="java.lang.String" column="art_singular" length="10" not-null="true" />

		<property name="nombreSingular" type="java.lang.String" column="nombre_singular" length="100" not-null="true" />

		<property name="articuloPlural" type="java.lang.String" column="art_plural" length="10" not-null="true" />

		<property name="nombrePlural" type="java.lang.String" column="nombre_plural" length="100" not-null="true" />

		<!-- Associations -->

	</class>

</hibernate-mapping>
