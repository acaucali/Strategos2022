<?xml version='1.0' encoding='utf-8'?>

<!DOCTYPE hibernate-mapping SYSTEM "../../../../hibernate-mapping-3.0.dtd" >

<!-- Modificado por: Kerwin Arias (28/08/2012) -->

<hibernate-mapping>

	<class name="com.visiongc.framework.model.ConfiguracionUsuario" table="afw_config_usuario">

		<composite-id name="pk" class="com.visiongc.framework.model.ConfiguracionUsuarioPK">
			<key-property name="usuarioId" column="usuario_id" type="java.lang.Long" />
			<key-property name="configuracionBase" column="configuracion_base" type="java.lang.String" length="200" />
			<key-property name="objeto" column="objeto" type="java.lang.String" length="100" />
		</composite-id>

		<property name="data" type="java.lang.String" column="data" />

	</class>
</hibernate-mapping>
